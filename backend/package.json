{
  "name": "expense-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Expense Tracker application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node scripts/migrate.js",
    "migrate:sequelize": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run migrate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'Backend build complete'",
    "health": "curl -f http://localhost:5000/api/health || exit 1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "joi": "^17.9.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "expense-tracker",
    "api",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT"
}
